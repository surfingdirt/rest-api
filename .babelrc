{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": false,
        "shippedProposals": true,
        "useBuiltIns": "usage",
        "targets": {
          "node": "current"
        }
      }
    ],
  ],
  "env": {
    "test": {
      "plugins": ["require-context-hook"]
    }
  }
}
